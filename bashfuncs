dfind(){
   find . -type f -exec grep $1 {} \; -ls
}
psisrunning(){
   ps -eaf |grep $1 | grep -v grep
}

setgithub(){
        git config --global user.name markmontymark
        git config --global user.email "markmontymark@yahoo.com"
}

mrkinit(){

export JAVA_HOME=/usr/local/java
export PATH=$JAVA_HOME/bin:$PATH
export ANT_HOME=/usr/local/ant
export PATH=$ANT_HOME/bin:$PATH
alias jenkins-start="cd $HOME/projects/jenkins; nohup java -Dorg.jenkinsci.plugins.gitclient.Git.useCLI=true -jar jenkins.war > jenkins.log 2>&1 &"
alias jenkins-stop='wget http://localhost:8080/exit'

export PATH=/home/mkoopman/vendor/rakudo-star-2013.05:$PATH
export PATH=$HOME/.alternatives:$PATH
export GOPATH=$HOME/go-workspace
export PATH=$PATH:$GOPATH/bin


}

